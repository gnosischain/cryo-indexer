# Blockchain RPC
ETH_RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY

# ClickHouse Cloud Configuration
CLICKHOUSE_HOST=your-instance.clickhouse.cloud
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=your_password
CLICKHOUSE_DATABASE=blockchain
CLICKHOUSE_PORT=8443
CLICKHOUSE_SECURE=true

# Indexer Configuration
CONFIRMATION_BLOCKS=20
POLL_INTERVAL=15
LOG_LEVEL=INFO
CHAIN_ID=1
START_BLOCK=0
MAX_BLOCKS_PER_BATCH=1000
RUN_MIGRATIONS=true

# Datasets to index (comma-separated, no spaces)
# Available options: 
# blocks,transactions,logs,contracts,native_transfers,traces,
# balance_diffs,code_diffs,nonce_diffs,storage_diffs,
# balance_reads,code_reads,storage_reads,
# erc20_transfers,erc20_metadata,erc20_balances,erc20_supplies,erc20_approvals,
# erc721_transfers,erc721_metadata
DATASETS=blocks,transactions,logs